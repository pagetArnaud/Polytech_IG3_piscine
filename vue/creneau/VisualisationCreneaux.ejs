<%- include('../commun/head'); %>
<link rel="stylesheet" href="/css/Visualisation_creneau.css">

<body>
<table class="tableau-style">
    <thead>
    <tr>
        <th>Nom/Pr√©nom</th>
        <th>Projet</th>
        <th>Date</th>
        <th>Salle</th>
        <th>Heure</th>
        <th>Groupe</th>

    </tr>
    </thead>
    <tbody>
    <% data.forEach(function(row) { %>
        <% var date = new Date(row.date); %>
        <tr>

            <td><%= date.getDate() %>/<%= date.getMonth() + 1 %>/<%= date.getFullYear() %></td>
            <td><%= row.salle %></td>

            <td><%= row.heureDebut %></td>


            <td>
                <% if (row.groupe){ %>
                    <%= row.groupe %>
                <% }else { %>
                    <p>LIBRE</p>
            </td>
            <% } %>
        </tr>
    <% }); %>
    </tbody>



</table>

</body>
<%- include('../commun/footer'); %>